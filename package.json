{
  "name": "stone-soup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": "",
  "author": "Cinnamon <cinnamon_bun_github@fastmail.com>",
  "homepage": "https://github.com/cinnamon-bun/stone-soup",
  "license": "AGPL-3.0",
  "scripts": {
    "clean": "rm -rf build coverage .nyc_output depchart/*",
    "build": "tsc",
    "start": "node build/main.js",
    "test": "tap -Rspecy --jobs=1 build/test/*.test.js",
    "build-and-test": "yarn build && yarn test",
    "watch-test": "onchange -i src/*.ts src/test/*.ts -- yarn build-and-test",
    "test-coverage": "yarn test -- --coverage-report=lcov",
    "prepublishOnly": "yarn run clean && yarn run build",
    "depchart": "depchart `find src | grep .ts` --exclude `find src | grep '/test/'` `find src | grep '/types/'` src/_old.ts src/utils.ts src/log.ts --rankdir LR -o depchart/depchart --open"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "@types/tap": "^14.10.2",
    "depchart": "^1.1.0",
    "onchange": "^7.1.0",
    "tap": "^14.11.0",
    "typescript": "^4.1.4"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "fast-deep-equal": "^3.1.3",
    "rfdc": "^1.2.0"
  }
}
